# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
SUPABASE_JWT_SECRET=your_supabase_jwt_secret_here

# Admin Configuration
ADMIN_SECRET_KEY=your_admin_secret_key_here

# Wallet Encryption (32-byte hex, 64 chars). Used to encrypt flow_private_key in wallet table.
# Must match between encrypt and decrypt. Set same as ADMIN_SECRET_KEY or generate: openssl rand -hex 32
WALLET_ENCRYPTION_KEY=your_wallet_encryption_key_here

# Sync Configuration
SYNC_INTERVAL=300
